extends layout

block content
  - if (messages.error)
    h1 Sorry, there was a problem.

    h2.message.error
      = messages.error

  - else
  #provider-list
    each provider, name in providers
      a.button(href=provider._entryPath)
        | Sign in with #{provider.title || name}
        img(class="button-img" src="/_doorman/icons/arrow-right.svg")
        
